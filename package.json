{
  "name": "angular2-boilerplate",
  "version": "1.0.0",
  "description": "Simple starting code to speed up development of Angular 2 projects.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briandennis/Angular2Boilerplate.git"
  },
  "author": "Brian Dennis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/briandennis/Angular2Boilerplate/issues"
  },
  "homepage": "https://github.com/briandennis/Angular2Boilerplate#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.13",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.25",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.11"
  }
}
